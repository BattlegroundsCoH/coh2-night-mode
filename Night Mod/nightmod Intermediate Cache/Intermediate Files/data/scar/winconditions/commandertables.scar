};
 
-- Replace the abilities
function Util_GetCommander(player)
 
    local faction = Player_GetRaceName(player);
 
    for k, v in pairs(g_commanderTable[faction]) do
        local match = 0;
        for i=1, #v do
            if (Player_HasAbility(player, v[i].remove)) then
                match = match + 1;
            end
        end
        if match == #v then
            return k;
        end
    end
 
    return nil;
 
end
 
function Util_ReplaceCommander(player, commander)
 
    local faction = Player_GetRaceName(player);
 
    for i=1, #g_commanderTable[faction][commander] do
        Util_ReplaceAbility(player, g_commanderTable[faction][commander][i].remove, g_commanderTable[faction][commander][i].add);
    end
 
end
 
function Util_ReplaceAbility(player, ability_remove, ability_add)
 
    -- Remove ability
    Player_SetAbilityAvailability( player, ability_remove, ITEM_REMOVED );
 
    -- Add ability (and incase we removed it, re-add it)
    Player_SetAbilityAvailability( player, ability_add, ITEM_DEFAULT  );
    Player_AddAbility( player, ability_add );
 
 
end
 
function _run_alaways()
 
    for i = 1, World_GetPlayerCount() do
 
        local player = World_GetPlayerAt(i);
 
        if g_checkedPlayers[Player_GetID(player)] == nil then
           
            local selected_commander = Util_GetCommander(player);
 
            if selected_commander ~= nil then
                Util_ReplaceCommander(player, selected_commander);
                g_checkedPlayers[Player_GetID(player)] = true;
            end
 
        end
 
    end
 
end