			{ remove = BP_GetAbilityBlueprint("flare_artillery"), add = BP_GetAbilityBlueprint("3690fed557b44fc68b9753c2f52ed775:flare_artillery") },
			{ remove = BP_GetAbilityBlueprint("command_panther"), add = BP_GetAbilityBlueprint("3690fed557b44fc68b9753c2f52ed775:command_panther") },
